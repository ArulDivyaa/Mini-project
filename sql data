USE school;
DROP TABLE IF EXISTS shopproducts ;
 CREATE TABLE  shopproducts (
	id int not null,
	prod_name varchar(200) NOT NULL,
	description text NOT NULL,
	price int NOT NULL,
	qty int NOT NULL,
    img text NOT NULL,
	PRIMARY KEY (id)
);

INSERT INTO shopproducts VALUES(1, "Smart Watch", "can see time", 5000, 10, "watch.jpg");
